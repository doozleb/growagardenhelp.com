RewriteEngine On
RewriteBase /

# If the request is not for a file or directory
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Rewrite /strategies to strategies.html
RewriteRule ^strategies/?$ strategies.html [L]

# Rewrite /values to values.html
RewriteRule ^values/?$ values.html [L]

# Rewrite root to index.html
RewriteRule ^$ index.html [L] 